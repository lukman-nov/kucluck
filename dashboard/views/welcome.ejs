    <!-- END OUTER CSS -->
    <!-- MY CSS -->
    <!-- END MY CSS -->
    <title>Welcome | <%= bot.user.username %></title>
    <link rel="icon" type="image/png" href="<%= bot.user.displayAvatarURL({size: 128}) %>">
  </head>
  <body>
    
  <script>
    Swal.fire({
      icon: 'success',
      iconColor: '#ddc3ff',
      title: 'Hallo, <%=user.username%>#<%=user.discriminator%>',
      showConfirmButton: false,
      timer: 3000,
      timerProgressBar: true,
      didOpen: () => {
        Swal.showLoading()
        const b = Swal.getHtmlContainer().querySelector('b')
        timerInterval = setInterval(() => {
          b.textContent = Swal.getTimerLeft()
        }, 100)
      },
      willClose: () => {
        clearInterval(timerInterval)
      }
    })
  </script>
  <img src="https://media.discordapp.net/attachments/901446802711142460/931714294654042162/newlkuclucklogok.png?width=575&height=480" alt="Logo" style="margin-top: 10%; position: absolute;">
  <img src="https://media.discordapp.net/attachments/901446802711142460/931875177497780284/newlkuclucklogokFlip.png?width=575&height=480" alt="Logo" style="margin-top: 10%; position: absolute; margin-left: 70%;">
  </body>
  <style>
    body {
      background-color: #1f1f20;
      color: #eee;
    }
    .swal2-popup {
      background: #272727 !important;
    }
    .swal2-success-circular-line-left {
      background: #272727 !important;
    }
    .swal2-success-circular-line-right {
      background: #272727 !important;
    }
    .swal2-success-fix {
      background: #272727 !important;
    }
    .swal2-icon.swal2-success.swal2-icon-show {
      border-color: #ddc3ff;
      color: #ddc3ff;
    }
    .swal2-icon.swal2-success [class^='swal2-success-line']{
      background-color: #ddc3ff;
    }
    .swal2-icon.swal2-success .swal2-success-ring{
      border: 0.25em solid #a08db9a2;
    }
    .swal2-loader {
      border-color: #ddc3ff transparent #ddc3ff transparent;
    }
  </style>
</html>